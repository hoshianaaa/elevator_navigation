<?xml version="1.0"?>

<launch>
  <arg name="robot_name" default="alpha"/>
  <arg name="init_pos_file" default="$(find orne_navigation_executor)/initial_pose_cfg/initial_pose.yaml"/>
  <arg name="start_floor" default="3"/>
  
  <include file="$(find ele_map_server)/launch/ele_map_server.launch">
    <arg name="start_floor" value="$(arg start_floor)"/>
  </include>

  <include file="$(find elevator_navigation_executor)/launch/move_base.launch">
    <arg name="robot_name" value="$(arg robot_name)"/>
    <arg name="odom_topic" value="combined_odom"/>
    <arg name="cmd_vel" value="icart_mini/cmd_vel"/>
  </include>

  <include file="$(find elevator_navigation_executor)/launch/localization.launch">
    <arg name="robot_name" value="$(arg robot_name)"/>
    <arg name="init_pos_file" value="$(arg init_pos_file)"/>
  </include>

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find orne_navigation_executor)/rviz_cfg/nav.rviz"/>

</launch>
